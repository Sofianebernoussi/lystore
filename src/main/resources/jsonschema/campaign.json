{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string"
    },
    "accessible": {
      "type": "boolean"
    },
    "description": {
      "type": [ "string", "null" ]
    },
    "groups": {
      "type": "array"
    },
    "image":{
      "type": [ "string", "null" ]
    },
    "purse_enabled": {
      "type": "boolean"
    },
    "priority_enabled": {
      "type": "boolean"
    }
  },
  "required": [
    "name",
    "description",
    "groups",
    "accessible",
    "purse_enabled"
  ]
}
